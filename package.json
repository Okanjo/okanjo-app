{
  "name": "@okanjo/okanjo-app",
  "version": "1.0.0",
  "description": "Okanjo application framework",
  "main": "okanjo-app.js",
  "scripts": {
    "test": "node_modules/.bin/mocha test",
    "cover": "rm -f coverage/coverage*.json && node_modules/.bin/istanbul cover node_modules/mocha/bin/_mocha -- -R spec && node_modules/.bin/istanbul report && echo \"Final Report:\" && node_modules/.bin/istanbul report text-summary",
    "hint": "node_modules/.bin/jshint ./*.js",
    "report": "npm run cover && npm run hint"
  },
  "repository": {
    "type": "git",
    "url": "https://okanjo.git.beanstalkapp.com/okanjo-dashboard.git"
  },
  "keywords": [
    "Okanjo",
    "Application",
    "Framework",
    "Reporting",
    "Environments",
    "Configuration"
  ],
  "author": "Okanjo Partners Inc",
  "license": "MIT",
  "devDependencies": {
    "istanbul": "^0.4.5",
    "jshint": "^2.8.0",
    "mocha": "^4.0.1",
    "should": "^13.1.3"
  },
  "dependencies": {
    "async": "^2.6.0",
    "boom": "^5.2.0",
    "raven": "^2.0.2"
  }
}
