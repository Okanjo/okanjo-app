{
  "name": "@okanjo/okanjo-app",
  "version": "1.0.0",
  "description": "Okanjo application framework",
  "main": "okanjo-app.js",
  "scripts": {
    "clean": "rm -rf .nyc_output coverage",
    "test": "node_modules/.bin/mocha -R spec test",
    "cover": "node_modules/.bin/nyc node_modules/mocha/bin/_mocha",
    "hint": "node_modules/.bin/jshint ./*.js",
    "report": "npm run cover && npm run hint"
  },
  "repository": {
    "type": "git",
    "url": "https://okanjo.git.beanstalkapp.com/okanjo-dashboard.git"
  },
  "keywords": [
    "Okanjo",
    "Application",
    "Framework",
    "Reporting",
    "Environments",
    "Configuration"
  ],
  "author": "Okanjo Partners Inc",
  "license": "MIT",
  "devDependencies": {
    "jshint": "^2.8.0",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "should": "^13.1.3"
  },
  "dependencies": {
    "async": "^2.6.0",
    "boom": "^5.2.0",
    "raven": "^2.0.2"
  },
  "nyc": {
    "reporter": [
      "text-summary",
      "html",
      "lcovonly"
    ]
  }
}
