{
  "name": "okanjo-app",
  "version": "2.0.0",
  "description": "Okanjo application framework",
  "main": "OkanjoApp.js",
  "scripts": {
    "clean": "rm -rf .nyc_output coverage",
    "test": "node_modules/.bin/mocha -R spec test",
    "cover": "node_modules/.bin/nyc node_modules/mocha/bin/_mocha",
    "lint": "node_modules/.bin/eslint .",
    "report": "npm run cover && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/okanjo/okanjo-app.git"
  },
  "keywords": [
    "Okanjo",
    "Application",
    "Framework",
    "Reporting",
    "Environments",
    "Configuration"
  ],
  "author": "Okanjo Partners Inc",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^5.15.2",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "should": "^13.2.3"
  },
  "dependencies": {
    "async": "^2.6.2",
    "boom": "^7.3.0",
    "raven": "^2.6.4"
  },
  "nyc": {
    "reporter": [
      "text-summary",
      "html",
      "lcovonly"
    ]
  }
}
